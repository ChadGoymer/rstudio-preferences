{
    "save_workspace": "never",
    "load_workspace": false,
    "always_save_history": false,
    "scroll_past_end_of_document": true,
    "enable_text_drag": false,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "warn_variable_defined_but_not_used": true,
    "style_diagnostics": true,
    "default_encoding": "UTF-8",
    "code_completion": "manual",
    "code_completion_other": "manual",
    "syntax_color_console": true,
    "windows_terminal_shell": "win-git-bash",
    "editor_theme": "Pastel On Dark",
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "insert_native_pipe_operator": true,
    "jobs_tab_visibility": "shown",
    "new_proj_git_init": true,
    "new_proj_use_renv": true,
    "save_files_before_build": true,
    "shiny_viewer_type": "browser",
    "spelling_dictionary_language": "en_GB",
    "pdf_previewer": "none",
    "check_for_updates": false,
    "initial_working_directory": "C:/Users/chadg/R",
    "restore_last_project": false,
    "auto_detect_indentation": true,
    "continue_comments_on_newline": true,
    "margin_column": 100,
    "warn_if_no_such_variable_in_scope": true,
    "indent_guides": "rainbowlines",
    "console_double_click_select": true,
    "cran_mirror": {
        "name": "Custom",
        "host": "Custom",
        "url": "https://packagemanager.rstudio.com/cran/2023-04-03",
        "secondary": "",
        "country": "",
        "changed": false
    },
    "show_doc_outline_rmd": true,
    "doc_outline_show": "sections_and_chunks",
    "default_project_location": "C:/Users/chadg/R"
}